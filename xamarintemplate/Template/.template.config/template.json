{
    "$schema": "http://json.schemastore.org/template",
    "description": "Xamarin.Forms app template with Prism.Forms, cache service, network reseliency, and more services to accelerate your app development.",
    "author": "IntelliAbb",
    "classifications": [
        "iOS",
        "Android",
        "Xamarin",
        "Prism"
    ],
    "icon":"https://intelliabbdotcom.files.wordpress.com/2022/08/image-1.png",
    "identity": "IntelliAbbXamarinTemplate",
    "name": "IntelliAbb Xamarin Template",
    "shortName": "ia-xamarin-app",
    "sourceName": "IntelliAbbXamarinTemplate",
    "tags":{
        "language":"C#",
        "type":"project"
    },
    "preferNameDirectory":true,
    "symbols": {
        "EnableCache": {
            "type": "parameter",
            "description": "Include IntelliAbb's simple cache service for in-memory and on-device cache",
            "datatype": "bool",
            "defaultValue": "true"
        },
        "EnablePolly": {
            "type": "parameter",
            "description": "Include network resiliency with Polly",
            "datatype": "bool",
            "defaultValue": "true"
        }
    }
}
